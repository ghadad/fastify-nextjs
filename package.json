{
  "name": "fastify-nextjs",
  "version": "4.0.0",
  "description": "React server side rendering support for Fastify with Next",
  "main": "index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "dev": "next dev",
    "test:client": "cross-env NODE_ENV=test jest",
    "test": "yarn test:client",
    "build": "yarn clean && next build",
    "clean": "rimraf out .next dist",
    "deploy": "yarn build && sls deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fastify/fastify-nextjs.git"
  },
  "keywords": [
    "fastify",
    "react",
    "next",
    "ssr",
    "server",
    "side",
    "rendering"
  ],
  "author": "Tomas Della Vedova - @delvedor (http://delved.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fastify/fastify-nextjs/issues"
  },
  "homepage": "https://github.com/fastify/fastify-nextjs#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "aws-serverless-express": "^3.3.5",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "fastify": "^2.0.0-rc.4",
    "fastify-compress": "^0.8.0",
    "fastify-helmet": "^3.0.0",
    "fastify-plugin": "^1.2.1",
    "next": "^8.0.0-canary.21",
    "next-offline": "^3.3.5",
    "path-match": "^1.2.4",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-domain-manager": "^2.6.13",
    "serverless-http": "^1.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "babel-jest": "^24.0.0",
    "babel-plugin-emotion": "^10.0.6",
    "cross-env": "^5.2.0",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "jest-dom": "^3.0.1",
    "prettier": "1.16.1",
    "pretty-quick": "^1.10.0",
    "react-addons-test-utils": "^16.0.0-alpha.3",
    "react-test-renderer": "^16.8.0-alpha.1",
    "react-testing-library": "^5.4.4",
    "rimraf": "^2.6.3",
    "serverless-offline": "^4.3.0",
    "tap": "^12.1.0"
  }
}
